{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to Chrome",
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Launch Edge",
            "type": "msedge",
            "request": "launch",
            "url": "http://127.0.0.1:3000/index.html",
            "webRoot": "${workspaceFolder}"
        }
    ]
}
